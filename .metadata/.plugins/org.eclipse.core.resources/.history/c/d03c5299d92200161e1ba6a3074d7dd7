package com.kitri.di.step11;

public class PersonDto {
	String name;
	String id;
	int money;
	double weight;
	
	public PersonDto() {
		super();
	}

	public PersonDto(String name) {
		super();
		this.name = name;
	}

	public PersonDto(String name, String id) {
		super();
		this.name = name;
		this.id = id;
	}

	public PersonDto(String name, int money) {
		super();
		this.name = name;
		this.money = money;
	}

	public PersonDto(String name, int money, double weight) {
		super();
		this.name = name;
		this.money = money;
		this.weight = weight;
	}

	public PersonDto(String name, String id, int money, double weight) {
		super();
		this.name = name;
		this.id = id;
		this.money = money;
		this.weight = weight;
	}
	
	
}
	
